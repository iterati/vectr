{
    "name": "VectrUI-clickResistant",
    "description": "VectrUI Chrome App",
    "version": "0.3.6",
    "manifest_version": 3,
    "app": {
        "background": {
            "scripts": ["app.js"]
        }
    },
    "icons": {
        "16": "assets/icons/logo-16.png",
        "128": "assets/icons/logo-128.png"
    },
    "permissions": [
        "serial",
        "storage",
        {"fileSystem": ["write", "directory", "retainEntries"]}
    ]
}
