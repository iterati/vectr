{
    "name": "VectrUI",
    "description": "VectrUI Chrome App",
    "version": "0.1",
    "manifest_version": 2,
    "app": {
        "background": {
            "scripts": ["vectrui.js"]
        }
    },
    "icons": {
        "16": "calculator-16.png",
        "128": "calculator-128.png"
    },
    "permissions": [
        "alert",
        "serial",
        "storage",
        {"fileSystem": ["write", "directory", "retainEntries"]}
    ]
}
